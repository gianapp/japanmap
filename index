<script>
function initMap () {
	var location = {lat: -25.363, lng: 131.044};
	var map = new google.maps.Map (document.getElementBtId(), {
		zoom: 4, 
		center: location
	});
var marker = new google.maps.Marker ({
	position: location,
	map: map
});
}	


</script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBMcR8cCOcsuFXKgfbfLbYWsznqOfNFef4&callback=initMap"></script>
